# Gemini API Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Model Configuration
VITE_DEFAULT_MODEL=gemini-3-flash-preview

# Optional: Feature Flags
VITE_ENABLE_MCP=true
VITE_ENABLE_FILESYSTEM_MODE=false

# Optional: Local backend (default http://127.0.0.1:7242). Override if backend runs elsewhere.
# VITE_API_BASE=http://127.0.0.1:7242

# Optional: When backend is not running, set to avoid connection errors (e.g. mock mode).
# VITE_LOCAL_MODE=true

# Optional: Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info
